{% extends 'base.html.twig' %}

{% block body %}
    <h1>Csvupload</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ csvUpload.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ csvUpload.nom }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{% if csvUpload.createdAt %}{{ csvUpload.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{% if csvUpload.updatedAt %}{{ csvUpload.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ csvUpload.type }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('csvupload_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('csvupload_edit', { 'id': csvUpload.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
